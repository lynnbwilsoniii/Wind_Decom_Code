	PROGRAM TESTPAK
C
C     SET UP PLOT (LINK WITH FASTRIP/LIB)
C
	COMMON /PPAK/ XMIN,XMAX,DELX,YMIN,YMAX,DELY,FILFAC
	DIMENSION XX(200),YY(200)
C
	CALL BPLTBGN(1,1024,' ','TESTPAK','LGRAPH',.6)
	CALL BPLTERM
	CALL BITPLTBGN('LGRAPH','PAGE 2')
	CALL THROW(358)
	DO 100 N = 1,100
	XX(N) = N
	YY(N) = XX(N)*(100.-XX(N))
  100	CONTINUE
	XMIN = 0.
	XMAX = 100.
	YMIN = 0.
	YMAX = 50.**2
	CALL LGRAPH(100,XX,YY)	
C
	CALL BPLTERM
	STOP
	END

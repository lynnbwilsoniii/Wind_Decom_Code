! items_rad2.db - WIND/WAVES Item Database: item definitions for RAD2 events
!
ITEM=DB_VERSION
	DATA_TYPE=CHARACTER
	TEXT=RAD2 2.0
	DESC=This is the current item database version number
	AUTHOR_DATE=19941207

ITEM=PACKET_ID       
	FIXED_VALUE=1:2      
	DESC=Fixed value of the packet ID for RAD2 in primary header
	AUTHOR_DATE=19910825

ITEM=RAD_ID
	FIXED_VALUE=1:2      
	DESC=Rad number (RAD2)
	AUTHOR_DATE=19960403
	XLATE=2,"RAD2"

ITEM=ANTENNA_FLAG    
	FIXED_VALUE=1:1
	DESC=Indication of antenna selection (must be Ey)
	AUTHOR_DATE=19931110
	XLATE=1,"Ey"

#include WIND_DBMS:items_rad.inc
#include WIND_DBMS:items_rad_phys.inc

! FREQUENCY AND CHANNEL INFORMATION
!
! Informational lists not tied to a particular instrument state. 
! Could be used in place of a data statement.
!

ITEM=FREQUENCY_COVERAGE_HZ_R4
	DATA_TYPE=REAL*4
	FILE='WIND_DBMS_AUX:rad2_frequencies.hz'
	DESC=List of frequencies in ascending order by channel (Hertz)
	AUTHOR_DATE=19941230

ITEM=CHANNEL_COVERAGE
	FILE='WIND_DBMS_AUX:rad2_frequencies.chan_num'
	DESC=Trivial list of channel numbers 0-255
	AUTHOR_DATE=19941230

!Items that are connected with particular instrument states

ITEM=CHANNEL_LIST_FILENAME_CHAR
	DATA_TYPE=CHARACTER
	VALIDATION=PACKET_SUBTYPE,MO,b1000
	VALIDATION=DPU_VERSION,GE,5
	TEXT=WIND_DBMS_AUX:rad2_list_mode.channels
	DESC=Filename of frequency/translate channel numbers for list mode
	AUTHOR_DATE=19941230

ITEM=CHANNEL_LIST
	VALIDATION=PACKET_SUBTYPE,MO,b1000
	VALIDATION=DPU_VERSION,GE,5
	FILE='WIND_DBMS_AUX:rad2_list_mode.channels'
	DESC=frequency/translate lists for list mode (channel #)
	AUTHOR_DATE=19941230

ITEM=FREQUENCY_LIST_HZ_R4
	DATA_TYPE=REAL*4
	VALIDATION=PACKET_SUBTYPE,MO,b1000
	VALIDATION=DPU_VERSION,GE,5
	FILE='WIND_DBMS_AUX:rad2_list_mode.hz'
	DESC=frequency/translate lists for list mode (Hertz)
	AUTHOR_DATE=19941230

ITEM=FREQUENCY_LIST_HZ_FILENAME_CHAR
	DATA_TYPE=CHARACTER
	VALIDATION=PACKET_SUBTYPE,MO,b1000
	VALIDATION=DPU_VERSION,GE,5
	TEXT=WIND_DBMS_AUX:rad2_list_mode.hz
	DESC=Filename of frequency/translate lists for list mode
	AUTHOR_DATE=19941209

ITEM=POINTER_LIST
	VALIDATION=PACKET_SUBTYPE,MO,b1000
	VALIDATION=DPU_VERSION,GE,5
	FILE='WIND_DBMS_AUX:rad2_list_mode.pointers'
	DESC=Pointer lists for list mode
	AUTHOR_DATE=19941230

ITEM=POINTER_LIST_FILENAME_CHAR
	DATA_TYPE=CHARACTER
	VALIDATION=PACKET_SUBTYPE,MO,b1000
	VALIDATION=DPU_VERSION,GE,5
	TEXT=WIND_DBMS_AUX:rad2_list_mode.pointers
	DESC=Filename of pointer lists for list mode
	AUTHOR_DATE=19941230

! RAD2 integration time items
!
ITEM=START_OFFSET_ZS_R4
	DATA_TYPE=REAL*4
	DESC=Time between EVENT start and the start of the first measurement
!	FIXED_VALUE=1:.023		!w/ 50,000.0 clock
	FIXED_VALUE=1:1150*"50KHZ_CLOCK_PERIOD_R4"
	AUTHOR_DATE=19940926

ITEM=START_OFFSET_SPRIME_R4
	DATA_TYPE=REAL*4
	DESC=Time between EVENT start and the start of the first measurement
!	FIXED_VALUE=1:.043		!w/ 50,000.0 clock
	FIXED_VALUE=1:2150*"50KHZ_CLOCK_PERIOD_R4"
	AUTHOR_DATE=19940926

ITEM=OFFSET_WITHIN_SET_R4
	DATA_TYPE=REAL*4
	DESC=Time between measurements in a set (seconds)
	FIXED_VALUE=1:0
	AUTHOR_DATE=19940926

ITEM=NUMBER_MEASURES_IN_SET
	DESC=Number of measurements in a set
	FIXED_VALUE=1:1
	AUTHOR_DATE=19940926

ITEM=OFFSET_TO_NEXT_SET_R4
	DATA_TYPE=REAL*4
	DESC=.063, the time between consecutive RAD2 sets (seconds)
!	FIXED_VALUE=1:.063		!w/ 50,000.0 clock
	FIXED_VALUE=1:3150*"50KHZ_CLOCK_PERIOD_R4"
	AUTHOR_DATE=19940926

ITEM=INTEGRATION_TIME_S_R4
	DATA_TYPE=REAL*4
        DESC=.020, the RAD2 S integration time (seconds)
!	FIXED_VALUE=1:.020		!w/ 50,000.0 clock
	FIXED_VALUE=1:1000*"50KHZ_CLOCK_PERIOD_R4"
	AUTHOR_DATE=19940926

ITEM=INTEGRATION_TIME_Z_R4
	DATA_TYPE=REAL*4
        DESC=.040, the RAD2 Z integration time (seconds)
!	FIXED_VALUE=1:.040		!w/ 50,000.0 clock
	FIXED_VALUE=1:2000*"50KHZ_CLOCK_PERIOD_R4"
	AUTHOR_DATE=19940926


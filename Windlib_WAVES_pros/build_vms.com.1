$! VMS command file to build the TESTMODULE DLM.
$
$
$ CC/NOLIST/EXTERN=STRICT_REFDEF/ANSI/FLOAT=IEEE_FLOAT -
	/IEEE_MODE=DENORM_RESULTS/NODEBUG -
	/INCLUDE=IDL_DIR:[EXTERNAL] TESTMODULE
$
$ if "''f$trnlnm("IDL_EXE")'" .eqs. "" THEN -
$	 DEFINE IDL_EXE IDL_DIR:[BIN.BIN_ALPHA]IDL.EXE/USER_MODE
$
$ link /share testmodule,sys$input/opt/exe=testmodule.exe
	IDL_EXE/share
	SYMBOL_VECTOR=(IDL_Load=PROCEDURE)
$ exit
